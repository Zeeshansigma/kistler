{% for block in section.blocks %}
  {% case block.type %}
    {% when "content-grid" %}
      {% include "section-content-grid" %}
    {% when "featured-product" %}
      {% include "section-featured-product" %}
    {% when "hero" %}
      {% include "section-hero" %}
    {% when "html-content" %}
      {% include "section-html-content" %}
    {% when "product-listing" %}
      {% include "section-product-listing" %}
    {% when "rod-finder" %}
      {% include "section-rod-finder" %}
    {% when "video" %}
      {% include "section-video" %}
    {% when "image-video-with-text" %}
      {% include "section-image-video-with-text" %}
    {% when "multicolumn" %}
      {% include "section-multicolumn-content" %}
    {% when "multicolumn-html" %}
      {% include "section-multicolumn-html" %}
    {% when "photo-gallery" %}
      {% include "section-photo-gallery" %}
    {% when "faq" %}
      {% include "section-faq" %}
    {% when "video-slideshow" %}
      {% include "section-video-slideshow" %}
  {% endcase %}
{% endfor %}

