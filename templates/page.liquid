<main class="content" role="main">
  <div class="container">
    {% include 'page-utility' %}
  </div>
  <div class="container page page--no-sidebar">
    <div class="page__content">
      {% if false %} {% include 'bss-title-store-locator' %} {% include 'bss-description-store-locator' %} {% else %} <h1>{{ page.title }}</h1> {% endif %}
      <hr>
      {% if page.content.size > 0 %}
        <div class="rte">
          {{ page.content }}
        </div>
      {% endif %}
    </div>
  </div>
</main>
