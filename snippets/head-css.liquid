{% include 'critical-css'%}
{% comment %} <link rel="stylesheet" href="{{ 'bundle.critical.css' | asset_url }}"> {% endcomment %}

{% if template contains 'product' %}
  {% include 'critical-product-css'%}
  {% comment %} <link rel="stylesheet" href="{{ 'bundle.product.css' | asset_url }}"> {% endcomment %}
{% endif %}

{% if template contains 'cart' %}
  <link rel="preload" as="style" onload="this.onload=null;this.rel='stylesheet'" href="{{ 'bundle.cart.css' | asset_url }}">
  <link rel="preload" as="style" onload="this.onload=null;this.rel='stylesheet'" href="{{ 'assembly-cart-helper.css' | asset_url }}">
  <noscript>
    <link rel="stylesheet" href="{{ 'bundle.cart.css' | asset_url }}">
    <link rel="stylesheet" href="{{ 'assembly-cart-helper.css' | asset_url }}">
  </noscript>
{% endif %}

{% if template contains 'blog' or template contains 'article' %}
  <link rel="preload" as="style" onload="this.onload=null;this.rel='stylesheet'" href="{{ 'bundle.blog.css' | asset_url }}">
  <noscript><link rel="stylesheet" href="{{ 'bundle.blog.css' | asset_url }}"></noscript>
{% endif %}

{% comment %}Main Theme CSS{% endcomment %}
<link rel="preload" as="style" onload="this.onload=null;this.rel='stylesheet'" href="{{ 'bundle.theme.css' | asset_url }}">
<noscript><link rel="stylesheet" href="{{ 'bundle.theme.css' | asset_url }}"></noscript>

{% comment %}Gift Card Order CSS{% endcomment %}
{% if template contains 'gift_card' %}
  {{ 'gift-card.css' | shopify_asset_url | stylesheet_tag }}
  <link rel="preload" as="style" onload="this.onload=null;this.rel='stylesheet'" href="{{ 'gift-card.css' | asset_url }}">
  <noscript><link rel="stylesheet" href="{{ 'gift-card.css' | asset_url }}"></noscript>
{% endif %}
